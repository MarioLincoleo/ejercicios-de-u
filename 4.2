Proceso orenador10000000000000
	Escribir "ingrese cantidad de dinero que quiera ordenar";
		leer moni;
	si moni>=20000 entonces
		b20<-trunc(moni/20000);
	Sino
		b20<-0;
	FinSi
	si (moni-20000*b20)>=10000 Entonces
		b10<-trunc((moni-20000*b20)/10000);
	Sino
		b10<-0;
	FinSi
	si (moni-20000*b20-10000*b10)>=5000 Entonces
		b5<-trunc((moni-20000*b20-10000*b10)/5000);
	Sino
		b5<-0;
	FinSi
	si (moni-20000*b20-10000*b10-5000*b5)>=2000 Entonces
		b2<-trunc((moni-20000*b20-10000*b10-5000*b5)/2000);
	Sino
		b2<-0;		
	FinSi
	si (moni-20000*b20-10000*b10-5000*b5-2000*b2)>=1000 Entonces
		b1<-trunc((moni-20000*b20-10000*b10-5000*b5-2000*b2)/1000);
	Sino
		b1<-0;
	FinSi
	si (moni-20000*b20-10000*b10-5000*b5-2000*b2-1000*b1)>=500 Entonces
		m500<-trunc((moni-20000*b20-10000*b10-5000*b5-2000*b2-1000*b1)/500);
	Sino
		m500<-0;		
	FinSi
	si (moni-20000*b20-10000*b10-5000*b5-2000*b2-1000*b1-500*m500)>=100 Entonces
		m100<-trunc((moni-20000*b20-10000*b10-5000*b5-2000*b2-1000*b1-500*m500)/100);
	Sino
		m100<-0;
	FinSi
	si (moni-20000*b20-10000*b10-5000*b5-2000*b2-1000*b1-500*m500-100*m100)>= 50 Entonces
		m50<-trunc((moni-20000*b20-10000*b10-5000*b5-2000*b2-1000*b1-500*m500-100*m100)/50);
	Sino
		m50<-0;
	FinSi
	si (moni-20000*b20-10000*b10-5000*b5-2000*b2-1000*b1-500*m500-100*m100-50*m50)>=10 Entonces
		m10<-trunc((moni-20000*b20-10000*b10-5000*b5-2000*b2-1000*b1-500*m500-100*m100-50*m50)/10);
	Sino
		m10<-0;
	FinSi
	Escribir "billetes de $20.000= ", b20;
	Escribir "billetes de $10.000= ", b10;
	Escribir "billetes de $5.000= ", b5;
	Escribir "billetes de $2.000= ", b2;
	Escribir "billetes de $1.000= ", b1;
	Escribir "billetes de $500= ", m500;
	Escribir "billetes de $100= ", m100;
	Escribir "billetes de $50= ", m50;
	Escribir "billetes de $10= ", m10;
	
FinProceso
